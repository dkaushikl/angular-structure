{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "test": "ng test --watch=false --code-coverage",
    "clean": "rimraf ./dist/",
    "build": "npm run clean && tsc",
    "reinstall": "npm cache clean && npm install",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json",
    "release": "npm run build && ng build --prod --build-optimizer --vendor-chunk --common-chunk",
    "analyze": "npm run release -- --stats-json && webpack-bundle-analyzer ./dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.6",
    "@angular/common": "^6.1.6",
    "@angular/compiler": "^6.1.6",
    "@angular/core": "^6.1.6",
    "@angular/forms": "^6.1.6",
    "@angular/http": "^6.1.6",
    "@angular/platform-browser": "^6.1.6",
    "@angular/platform-browser-dynamic": "^6.1.6",
    "@angular/pwa": "^0.7.5",
    "@angular/router": "^6.1.6",
    "@angular/service-worker": "^6.1.6",
    "@aspnet/signalr": "^1.0.3",
    "angular2-ladda": "^2.0.0",
    "angular2-recaptcha": "^1.1.0",
    "core-js": "^2.5.7",
    "datatables.net": "^1.10.19",
    "datatables.net-dt": "^1.10.19",
    "jquery": "^3.3.1",
    "ng-bootstrap-form-validation": "^2.1.0",
    "ngx-bootstrap": "^3.0.1",
    "ngx-perfect-scrollbar": "^6.3.1",
    "ngx-qrcode2": "0.0.9",
    "ngx-toastr": "^9.0.2",
    "rxjs": "^6.3.1",
    "sweetalert2": "^7.26.14",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.5",
    "@angular/cli": "~6.1.5",
    "@angular/compiler-cli": "^6.1.6",
    "@angular/language-service": "^6.1.6",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.9.4",
    "bundlesize": "^0.17.0",
    "codelyzer": "~4.4.4",
    "rimraf": "^2.6.2",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2",
    "webpack-bundle-analyzer": "^2.13.1"
  }
}
